{
  "functions": {
    "api/webhook.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30,
      "memory": 512
    },
    "api/convert.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 300,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/convert",
      "dest": "/api/convert.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/webhook.js"
    }
  ]
}
